     }).pause()\n    )\n  },[]);\n\n  useEffect(() => {\n    let value = 0;\n    if(state === 'open') {\n      setVisibility(true);\n      value = 0.8\n    } else {\n      setVisibility(false);\n      value = 0;\n    }  \n    TweenMax.to(menu, 0.6, {\n      opacity: value,\n      ease: 'Power2.easeOut'\n    }).play()\n  })\n\n  const visibleStyle = (state === 'open' && visibiiity) ? '' : styles.hide;\n\n  return (\n    <div className={`menu ${styles.menu} ${visibleStyle}`} ref={element => { menu = element }}>\n      <ul>\n        <li><Link href={`/`} onClick={onClick}>top</Link></li>\n        <li><a href={`https://github.com/moxuse/cv/blob/master/README.md`} target='blank'>cv</a></li>\n        <li><Link href={`/work`} onClick={onClick}>work</Link></li>\n        <li><Link href={`/blog`} id={1} onClick={onClick}>blog</Link></li>\n      </ul>\n    </div>\n  );\n}\n\nexport default Menu;\n\n// Menu.defaultProps = {\n//   onClose: undefined\n// };\n"]},"metadata":{},"sourceType":"module"}